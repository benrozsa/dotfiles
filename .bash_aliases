# -- Configs
alias zshconfig="vi ~/.zshrc"
alias aliasconfig="vi ~/.bash_aliases"
alias vimconfig="vi ~/.vimrc"
alias sshconfig="vi ~/.ssh/config"
alias gitconfig="vi ~/.gitconfig"
alias fishconfig="vi ~/.config/fish/config.fish"

# Shell
alias ls='ls -F'
alias ll='ls -lh'
alias l='ls -la'

# Python -- Dangerous!!
alias python="python3"
alias pip="pip3"

# Git
alias gut="git"
alias got="git"
alias fit=="git"
alias s="git status"
alias a="git add ."
alias d="git diff HEAD"
alias pu="git push"
alias p="git pull --rebase --autostash"
alias lo="git log"
alias lop="git log -p"
alias br="git br -a"
alias f="git fetch"
alias gs="git stash"
alias gsp="git stash pop"

# Yarn
alias y="yarn"
alias ys="yarn start"
alias yd="yarn dev"

# Other
alias grep='grep --color=auto'
alias c="clear && printf '\e[3J'"
