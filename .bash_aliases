# -- Configs
alias zshconfig="vi ~/.zshrc"
alias aliasconfig="vi ~/.bash_aliases"
alias vimconfig="vi ~/.vimrc"
alias sshconfig="vi ~/.ssh/config"
alias gitconfig="vi ~/.gitconfig"

# Python -- Dangerous!!
alias python="python3"
alias pip="pip3"

# Git
alias gut="git"
alias got="git"
alias gti="git"
alias fit=="git"
alias s="git status"
alias a="git add ."
alias d="git diff HEAD"
alias p="git push"
alias pu="git pull"
alias lo="git log"
alias lop="git log -p"
alias br="git br -a"
alias f="git fetch"

# Other
alias dfh='df -h | grep "/dev/"'
alias grep='grep --color=auto'
alias c="clear && printf '\e[3J'"

# -- Maven --
alias ci='mvn clean install'
alias cl='mvn clean install -Dmaven.test.skip'

# -- Working Directories --
alias ws='cd ~/WebstormProjects'
alias is='cd ~/IdeaProjects'
alias github='cd ~/Github'
alias hs='cd ~/HobbyProjects'
